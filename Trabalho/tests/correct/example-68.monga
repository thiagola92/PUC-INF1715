	@'\n';
	@!(b==true);
	@'\n';
	@!(b==false);
	@'\n';

	@!a==true;
	@'\n';
	@!a==false;
	@'\n';
	@!b==true;
	@'\n';
	@!b==false;
	@'\n';
}