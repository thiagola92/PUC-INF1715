
DEFINE_FUNCTION
    IDENTIFIER - TYPE_ARRAY - TYPE_CHARACTER - content: main
    BLOCK
        VARIABLE_LIST
            DEFINE_VARIABLE - TYPE_INTEGER
                IDENTIFIER - TYPE_ARRAY - TYPE_CHARACTER - content: x1
                TYPE_INTEGER - TYPE_INTEGER
            DEFINE_VARIABLE - TYPE_FLOAT
                IDENTIFIER - TYPE_ARRAY - TYPE_CHARACTER - content: x2
                TYPE_FLOAT - TYPE_FLOAT
        COMMAND_LIST
            ASSIGNMENT
                VARIABLE
                    IDENTIFIER - TYPE_ARRAY - TYPE_CHARACTER - content: x1
                EXPRESSION_ADD
                    DATA_INTEGER - TYPE_INTEGER - content: 1
                    EXPRESSION_CAST
                        DATA_FLOAT - TYPE_FLOAT - content: 3.000000
                        TYPE_INTEGER
            ASSIGNMENT
                VARIABLE
                    IDENTIFIER - TYPE_ARRAY - TYPE_CHARACTER - content: x2
                EXPRESSION_ADD
                    EXPRESSION_CAST
                        DATA_INTEGER - TYPE_INTEGER - content: 1
                        TYPE_FLOAT
                    DATA_FLOAT - TYPE_FLOAT - content: 3.000000
