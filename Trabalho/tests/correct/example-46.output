
DEFINE_FUNCTION
    IDENTIFIER - TYPE_ARRAY - TYPE_CHARACTER - content: main
    BLOCK
        DEFINE_VARIABLE - TYPE_ARRAY - TYPE_INTEGER
            IDENTIFIER - TYPE_ARRAY - TYPE_CHARACTER - content: a
            TYPE_ARRAY - TYPE_INTEGER
                TYPE_INTEGER - TYPE_INTEGER
        COMMAND_LIST
            ASSIGNMENT - TYPE_ARRAY - TYPE_INTEGER
                VARIABLE - TYPE_ARRAY - TYPE_INTEGER
                    IDENTIFIER - TYPE_ARRAY - TYPE_CHARACTER - content: a
                NEW_ARRAY - TYPE_ARRAY - TYPE_CHARACTER
                    TYPE_CHARACTER - TYPE_CHARACTER
                    DATA_INTEGER - TYPE_INTEGER - content: 1
            IF
                EXPRESSION_EQUAL - TYPE_BOOLEAN
                    ARRAY_POSITION - TYPE_INTEGER
                        VARIABLE - TYPE_ARRAY - TYPE_INTEGER
                            IDENTIFIER - TYPE_ARRAY - TYPE_CHARACTER - content: a
                        DATA_INTEGER - TYPE_INTEGER - content: 0
                    DATA_CHARACTER - TYPE_INTEGER - content: a
                BLOCK
